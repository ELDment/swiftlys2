{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/refs/heads/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "files": [
            "managed/managed.csproj"
          ]
        }
      ],
      "output": "api",
      "outputFormat": "apiPage"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.md"
        ]
      },
      {
        "files": [
          "articles/**.md"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default"
    ]
  }
}